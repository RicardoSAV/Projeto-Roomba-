
#include <SoftwareSerial.h>

SoftwareSerial Serial2(2, 3); // RX, TX
void setup() {
  // initialize both serial ports:
  
  
 // Serial.begin(115200);
 
 Serial2.begin(115200);
 // if (Serial.available()) {
   
    Serial2.write(128);
   delay(1000);
   
   Serial2.write(7);
   delay(1000);
  
    Serial.write(132);
   delay(1000);
  
   
//}
}
void loop() {
 Serial2.write(146);
   delay(100);
 Serial2.write(0);
   delay(100);
   Serial2.write(128);
   delay(100);
     Serial2.write(130);
   delay(100);
   
   
   delay(500);
  
   Serial2.write(146);
   delay(100);
 Serial2.write(0);
   delay(100);
   Serial2.write(64);
   delay(100);
   Serial2.write(0);
   delay(100);
   Serial2.write(0);
   delay(100);
 
  
  
  }

  
